{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "next/core-web-vitals", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "off",
      "ignorePackages",
      {
        "js": "always"
      }
    ],
    "import/no-unresolved": "off",
    "arrow-body-style": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "plugins": ["prettier"]
}
